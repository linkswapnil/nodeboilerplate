{
	"name": "sample-project",
	"version": "0.0.0",
	"private": true,
	"main": "./bin/www",
	"engines": {
		"node": "9.5.0",
		"npm": "5.6.0"
	},
	"scripts": {
		"start": "babel-node --presets es2015 ./bin/www",
		"lint": "xo --fix",
		"build": "npx webpack",
		"test": "mocha --compilers js:babel-core/register ./**/*.spec.js",
		"coverage": "nyc npm test"
	},
	"nyc": {
		"exclude": [
			"**/*.spec.js",
			"node_modules"
		],
		"include": [
			"mapppers/**/*.js",
			"middlewares/**/*.js",
			"routes/**/*.js",
			"services/**/*.js",
			"utils/**/*.js"
		],
		"reporter": [
			"lcov",
			"text-summary"
		]
	},
	"eslintConfig": {
		"parser": "babel-eslint",
		"env": {
			"node": true,
			"es6": true
		},
		"globals": {
			"$": true,
			"document": true,
			"qq": true,
			"describe": true,
			"it": true
		},
		"extends": [
			"xo/esnext",
			"./node_modules/xo/config/plugins.js"
		]
	},
	"xo": {
		"globals": [
			"$",
			"bookProposal",
			"document",
			"qq",
			"describe",
			"it"
		]
	},
	"dependencies": {
		"body-parser": "~1.18.3",
		"cookie-parser": "~1.4.3",
		"cookie-session": "^1.3.3",
		"csurf": "~1.9.0",
		"debug": "~2.6.9",
		"dotenv": "~6.0.0",
		"express": "~4.16.0",
		"express-handlebars": "~3.0.0",
		"express-hbs": "~1.0.4",
		"express-rate-limit": "~3.0.1",
		"express-session": "^1.16.2",
		"express-sslify": "~1.2.0",
		"handlebars": "^4.1.2",
		"hbs": "^4.0.4",
		"helmet": "~3.13.0",
		"http-errors": "~1.6.2",
		"moment": "^2.29.2",
		"morgan": "^1.9.1",
		"node-fetch": "~2.2.0",
		"pm2": "^3.5.0",
		"utf8-binary-cutter": "^0.9.2"
	},
	"babel": {
		"presets": [
		  "es2015"
		],
		"sourceMaps": true
	  },
	"devDependencies": {
		"babel-core": "~6.26.3",
		"babel-cli": "^6.0.0",
		"babel-eslint": "~8.2.6",
		"babel-loader": "~7.1.5",
		"babel-polyfill": "~6.26.0",
		"babel-preset-env": "~1.7.0",
		"babel-preset-es2015": "~6.24.1",
		"babel-preset-react": "~6.24.1",
		"babel-preset-stage-0": "~6.24.1",
		"chai": "^4.2.0",
		"chai-http": "^4.3.0",
		"clean-webpack-plugin": "~0.1.19",
		"css-loader": "~1.0.0",
		"eslint": "^5.16.0",
		"eslint-config-xo": "~0.23.0",
		"eslint-plugin-ava": "~5.0.0",
		"eslint-plugin-import": "~2.13.0",
		"eslint-plugin-no-use-extend-native": "~0.3.12",
		"eslint-plugin-node": "~7.0.1",
		"eslint-plugin-promise": "~3.8.0",
		"eslint-plugin-unicorn": "~4.0.3",
		"extract-loader": "~2.0.1",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "~1.1.11",
		"handlebars-loader": "~1.7.0",
		"html-webpack-plugin": "~3.2.0",
		"mini-css-extract-plugin": "~0.4.1",
		"mocha": "~5.2.0",
		"node-sass": "^4.12.0",
		"nyc": "~12.0.2",
		"postcss-loader": "~2.1.6",
		"sass-loader": "~7.0.3",
		"sinon": "~7.0.0",
		"start-server-webpack-plugin": "~2.2.5",
		"style-loader": "~0.21.0",
		"webpack": "~4.16.2",
		"webpack-cli": "^3.3.7",
		"xo": "^0.24.0"
	}
}
